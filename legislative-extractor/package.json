{
  "name": "legislative-extractor",
  "version": "1.0.0",
  "description": "Extract and process legislative/statutory data from XML sources",
  "type": "module",
  "main": "src/main.js",
  "scripts": {
    "start": "node src/main.js",
    "extract": "node src/main.js --step=extract",
    "load": "node src/main.js --step=load",
    "analyze-hierarchy": "node src/analyze_hierarchy.js",
    "test": "node --test"
